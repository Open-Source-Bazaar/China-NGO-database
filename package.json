{
  "name": "@open-source-bazaar/china-ngo-database",
  "version": "0.6.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@arshiash80/strapi-plugin-iconhub": "^1.1.1",
    "@ckeditor/strapi-plugin-ckeditor": "^1.1.1",
    "@strapi/plugin-cloud": "~5.23.5",
    "@strapi/plugin-color-picker": "~5.23.5",
    "@strapi/plugin-documentation": "~5.23.5",
    "@strapi/plugin-users-permissions": "~5.23.5",
    "@strapi/strapi": "~5.23.5",
    "better-sqlite3": "^12.2.0",
    "cross-env": "^10.0.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "strapi-location-picker": "^0.0.2",
    "strapi-plugin-config-sync": "^3.1.2",
    "strapi-plugin-multi-select": "^2.1.1",
    "strapi-provider-cloudflare-r2": "^0.3.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/node": "^22.18.6",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "husky": "^9.1.7",
    "koajax": "^3.1.2",
    "lint-staged": "^16.1.6",
    "mobx-restful": "^2.1.2",
    "mobx-strapi": "^0.7.5",
    "prettier": "^3.6.2",
    "swagger-typescript-api": "^13.2.11",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "web-utility": "^4.6.1",
    "xlsx": "^0.18.5",
    "zx": "^8.8.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "core-js-pure",
      "esbuild",
      "sharp"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{md,json,yml,js,mjs,ts,tsx}": "prettier --write"
  },
  "scripts": {
    "install": "pnpx git-utility download https://github.com/Open-Source-Bazaar/key-vault main China-NGO-database  ||  true",
    "prepare": "(husky || true)  &&  (cp -u .env.example .env || true)",
    "test": "lint-staged",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",
    "strapi": "strapi",
    "clean": "rm -rf .cache/ .strapi/ dist/",
    "develop": "strapi develop",
    "make-type": "cd src/extensions/documentation/  &&  tsx make-type documentation/1.0.0/full_documentation.json ../../../types/client/",
    "build-type": "rm -rf types/client/  &&  npm run make-type",
    "build": "npm run clean  &&  strapi ts:generate-types  &&  cross-env NODE_ENV=production  strapi build",
    "start": "cross-env NODE_ENV=production  strapi start",
    "pack-image": "docker build -t open-source-bazaar/china-ngo-database .",
    "database": "docker rm -f postgres-server  &&  docker run --name postgres-server -v ./.data:/var/lib/postgresql/data/ -e POSTGRES_PASSWORD=tobemodified -p 5432:5432 -d postgres",
    "container": "docker rm -f strapi-server  &&  docker run --name strapi-server -p 1337:1337 -d open-source-bazaar/china-ngo-database",
    "deploy": "strapi deploy",
    "console": "strapi console"
  }
}
